{
    "name": "marcop/php72-to-82-rector",
    "description": "A project to verify the transition from PHP 7.2 to 8.2 using Rector.",
    "require": {
        "php": "^7.2 || ^8.0",
        "rector/rector": "^0.12.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "rector/rector": "^0.12.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}